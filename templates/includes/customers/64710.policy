prefix-list cloud4 {
    172.16.0.0/21;
    185.15.56.0/24;
    208.80.155.88/29;
}
policy-statement BGP_cloud_in {
    term address {
        from {
            family inet;
            protocol bgp;
            prefix-list-filter cloud4 orlonger;
        }
        then next policy;
    }
    then reject;
}
