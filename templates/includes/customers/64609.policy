prefix-list dse-k8s-pod-ips4 {
    10.67.24.0/21;
}
prefix-list dse-k8s-pod-ips6 {
    2620:0:861:302::/64;
}
policy-statement dse-k8s_import {
    term pod_ips4 {
        from {
            family inet;
            protocol bgp;
            prefix-list-filter dse-k8s-pod-ips4 longer;
        }
        then accept;
    }
    term pod_ips6 {
        from {
            family inet6;
            protocol bgp;
            prefix-list-filter dse-k8s-pod-ips6 longer;
        }
        then accept;
    }
    then reject;
}
